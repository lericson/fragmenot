#!/usr/bin/make -f 

python ?= python3
var  ?= var
pat  := $(var)/runs_*/run_*
runs := $(wildcard $(pat))

$(info Pattern $(pat) matches $(words $(runs)) runs)


MAKEFLAGS += -k
PARAME_VERBOSE ?= false
export

%/.plot_cache.npz: ./scripts/precompute_plot_cache.py %/mesh.npz
	$(python) $< $(@D) 

all: $(runs:%=%/.plot_cache.npz)
	@echo done

.PHONY: all
.DEFAULT: all
